{
  "name": "ewon-muswk",
  "version": "1.0.0",
  "description": "This application will watch Elon Musk's tweets, then they will be uwu-fied and posted as their own tweet.",
  "license": "ISC",
  "author": {
    "name": "Sjors van Holst"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Schotsl/Iris-server"
  },
  "bugs": {
    "url": "https://github.com/Schotsl/Iris-server/issues"
  },
  "main": "index.js",
  "devDependencies": {
    "@types/node": "^13.11.1",
    "nodemon": "^2.0.3",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "twitter": "^1.7.1",
    "uwufier": "^1.1.0"
  },
  "scripts": {
    "serve": "nodemon",
    "build": "tsc && node dist/index.js"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run build",
    "ext": "ts"
  }
}
